{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/txt", "dest": "/api/php.txt" },
    { "src": "/ini", "dest": "/api/php.ini" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ],
  "builds": [
    {
      "src": "index.php",
      "use": "vercel-php@0.6.0"
    }
  ]
}
